{
  "name": "spryker/availability-offer-connector",
  "type": "library",
  "description": "AvailabilityOfferConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/availability": "^9.0.0",
    "spryker/decimal-object": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/offer-extension": "^1.0.0",
    "spryker/store": "^1.5.0",
    "spryker/transfer": "^3.18.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
